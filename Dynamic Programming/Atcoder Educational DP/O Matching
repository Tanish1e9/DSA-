// ye dhyan rakhna ki 64 tk hi ho agar elements to ek single integer hi vis array ki tarah kaam kr skta 
// Yaha i ko bhi dp me daal doge to accept ho jayega no problem but ni jroorat hai kyunki
// Yaha men hmesha increasing order me hi assign ho rhe h
// to kisi bhi waqt vis ko dekhke (usme jitne bhi set bits hai), unse we can determine ki kitne men visited honge
// aur wo men shuruwat waale hi honge because recursion aise work kr rha h
// mtlb first man ko assign kie bina to aage hi ni badhenge na
// similarly second ko assign kie bina aage hi ni badhenge na
// to aisa ho ni skta ki kuch set of visited women ho with different sets of visited men, wo hmesha shuruwat waale hi honge

#include<bits/stdc++.h>
using namespace std;
#define int long long
const int mod = 1e9+7;
int f(int i,auto&arr,int vis,auto&memo){
    if(i<0) return 1;
    if(memo[vis]!=-1) return memo[vis];
    int ans=0;
    for(int j=0;j<arr.size();j++){
        if(arr[i][j] && (vis & (1<<j))==0){
            ans = (ans+f(i-1,arr,vis|(1<<j),memo))%mod;
        }
    }
    return memo[vis]=ans;
}
int func(auto&arr,int n){
    vector<int>memo(1<<21,-1);
    int vis = 0;
    return f(n-1,arr,vis,memo);
}
signed main(){
    ios_base::sync_with_stdio(0);
    cout.tie(0);
    cin.tie(0);
    int n;
    cin >> n;
    vector<vector<int>>arr(n,vector<int>(n,0));
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cin >> arr[i][j];
        }
    }
    cout << func(arr,n) << endl;
    return 0;
}
