CSES link: https://cses.fi/problemset/task/1654/


// Ye SOS DP hai kind of sikh lo
// Vivek gupta ka explanation accha h usko dekh lena


#include <bits/stdc++.h>
using namespace std;
#define int long long
 
signed main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int n;
    cin >> n;
    const int N = 20;
    vector<int> dp(1<<N,0);
    vector<int> arr(n);
    for(int i=0;i<n;i++) {
        cin >> arr[i];
        dp[arr[i]]++;
    }
 
    for(int i=0;i<N;i++) {
        for(int mask=0;mask<(1<<N);mask++) {
            if(mask & (1<<i)) {
                dp[mask] += dp[mask ^ (1<<i)];
            }
        }
    }
 
    vector<vector<int>> ans(n,vector<int>(3));
    for(int i=0;i<n;i++) {
        ans[i][0] = dp[arr[i]];
        ans[i][2] = n-dp[((1<<N)-1) ^ arr[i]];
    }
 
    for(int i=N-1;i>=0;i--) {
        for(int mask = (1<<N)-1;mask>=0;mask--) {
            if(mask & (1<<i)) {
                dp[mask] -= dp[mask ^ (1<<i)];
            }
        }
    }
 
    for(int i=0;i<N;i++) {
        for(int mask=(1<<N)-1;mask>=0;mask--) {
            if(!(mask & (1<<i))) {
                dp[mask] += dp[mask ^ (1<<i)];
            }
        }
    }
 
    for(int i=0;i<n;i++) {
        ans[i][1] = dp[arr[i]];
    }
 
    for(int i=0;i<n;i++) {
        for(int j:ans[i]) {
            cout << j << " ";
        }
        cout << "\n";
    }
 
    return 0;
}

