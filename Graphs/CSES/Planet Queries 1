CSES link: https://cses.fi/problemset/task/1750/

#include<bits/stdc++.h>
using namespace std;
#define int long long
void populate(int n, auto &next) {
	int k=1;
	while(k<=29) {
		for(int i=1;i<=n;i++) {
			int mid = next[k-1][i];
			next[k][i] = next[k-1][mid];
		}
		k++;
	}
}
signed main() {
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	cout.tie(nullptr);
	int n, q;
	cin >> n >> q;
	vector<vector<int>> next(30,vector<int>(n+1));
	for(int i=1;i<=n;i++) {
		cin >> next[0][i];
	}
	populate(n, next);
	while(q--) {
		int x, k;
		cin >> x >> k;
		for(int i=29;i>=0;i--) {
			if((1<<i) & k) {
				x = next[i][x];
			}
		}
		cout << x << "\n";
	}
	return 0;
}
