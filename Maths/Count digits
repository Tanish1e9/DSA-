GFG link: https://www.geeksforgeeks.org/problems/count-digits5716/1

    // ek to ye tarika hota digits extract krne ka
    int evenlyDivides(int n){
        int ans=0;
        int x=n;
        while(n>0){
            int d= n%10;
            if(d!=0 && x%d==0) ans++;
            n/=10;
        }
        return ans;
    }

    // ek direct tarika ye h
    return int(log10(n))+1;
