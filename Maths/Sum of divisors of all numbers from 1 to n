GFG link: 

    // M-1 Brute O(n root n) TLE
    long long sum(int n){
        long long ans=0;
        for(long long i=1;i<=n/i;i++){
            if(n%i!=0) continue;
            if(i==n/i) ans+=i;
            else ans+= i + n/i;
        }
        return ans;
    }
    long long sumOfDivisors(int N)
    {
        long long ans=0;
        for(long long i=1;i<=N;i++){
            ans+=sum(i);
        }
        return ans;
    }
