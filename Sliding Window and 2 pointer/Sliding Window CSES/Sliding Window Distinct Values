CSES link: https://cses.fi/problemset/task/3222

// Yaha dekhna achanak se constraints thoda kam ho gya h
// kyunki map lagana hi iska optimal solution h

#include<bits/stdc++.h>
using namespace std;
#define int long long
signed main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int n, k;
	cin >> n >> k;
	vector<int> arr(n);
	for(int i=0;i<n;i++) {
		cin >> arr[i];
	}
	map<int,int> freq;
	for(int i=0;i<n;i++) {
		// Add element 
		freq[arr[i]]++;
 
		int l = i-k+1;
		if(l>=0) {
			// window exists
			// take ans
			cout << freq.size() << " ";
			// remove l
			freq[arr[l]]--;
			if(freq[arr[l]]==0) freq.erase(arr[l]);
		}
	}
	cout << endl;
	return 0;
}


